<head>    <meta charset="UTF-8" />    <meta name="viewport" content="width=device-width, initial-scale=1.0" />    <title>WEARiT | Home</title>    <link        href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"        rel="stylesheet"    />    <link        rel="stylesheet"        href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"    />    <link rel="stylesheet" href="/css/home.css" /></head><body>    <%- include('../partials/header') %>    <section class="hero-banner position-relative m-0 p-0">        <div class="container-fluid px-0">            <div class="position-relative">                <div                    id="heroCarousel"                    class="carousel slide"                    data-bs-ride="carousel"                    data-bs-interval="5000"                >                    <div class="carousel-indicators">                        <button                            type="button"                            data-bs-target="#heroCarousel"                            data-bs-slide-to="0"                            class="active"                            aria-current="true"                            aria-label="Slide 1"                        ></button>                        <button                            type="button"                            data-bs-target="#heroCarousel"                            data-bs-slide-to="1"                            aria-label="Slide 2"                        ></button>                        <button                            type="button"                            data-bs-target="#heroCarousel"                            data-bs-slide-to="2"                            aria-label="Slide 3"                        ></button>                    </div>                    <a href="/products">                        <div class="carousel-inner">                            <div class="carousel-item active">                                <img                                    src="/images/banner9.jpg"                                    class="d-block w-100"                                    alt="Shirts Collection"                                />                            </div>                            <div class="carousel-item">                                <img                                    src="/images/banner5.jpg"                                    class="d-block w-100"                                    alt="Fashion Collection"                                />                            </div>                            <div class="carousel-item">                                <img                                    src="/images/banner10.jpg"                                    class="d-block w-100"                                    alt="Summer Collection"                                />                            </div>                        </div>                    </a>                    <button                        class="carousel-control-prev"                        type="button"                        data-bs-target="#heroCarousel"                        data-bs-slide="prev"                    >                        <span                            class="carousel-control-prev-icon"                            aria-hidden="true"                        ></span>                        <span class="visually-hidden">Previous</span>                    </button>                    <button                        class="carousel-control-next"                        type="button"                        data-bs-target="#heroCarousel"                        data-bs-slide="next"                    >                        <span                            class="carousel-control-next-icon"                            aria-hidden="true"                        ></span>                        <span class="visually-hidden">Next</span>                    </button>                </div>            </div>        </div>    </section>    <section class="new-arrivals py-4 py-md-5">        <div class="container">            <h2 class="text-center mb-2 mb-md-3">New Arrivals</h2>            <p class="text-center text-muted mb-3 mb-md-5 px-3 px-md-0">                Check out our latest collection of trendy and stylish products.                Find your perfect match from our new arrivals.            </p>            <div class="row row-cols-2 row-cols-md-3 row-cols-lg-4 g-3 g-md-4">                <% if (typeof newArrivals !== 'undefined' && newArrivals.length                > 0) { %> <% newArrivals.forEach(function(product) { var                mainImage = product.images.find(img => img.isMain) ||                product.images[0] || { url: '/images/placeholder.jpg' }; var                defaultVariant = product.variants && product.variants.length > 0                ? product.variants[0] : null; var hasDiscount = product.offer >                0 && defaultVariant; var originalPrice = defaultVariant ?                defaultVariant.varientPrice : 0; var salePrice = defaultVariant                ? defaultVariant.salePrice : 0; var totalStock =                product.variants.reduce((sum, variant) => sum +                variant.varientquatity, 0); var rating = product.ratings ?                product.ratings.average : 0; var ratingCount = product.ratings ?                product.ratings.count : 0; %>                <div class="col">                    <div class="card product-card h-100 border-0 shadow-sm">                        <a                            href="/products/<%= product._id %>"                            class="text-decoration-none"                        >                            <div class="product-img-container">                                <img                                    src="<%= mainImage.url %>"                                    class="card-img-top"                                    alt="<%= product.name %>"                                />                            </div>                            <div class="card-body p-3">                                <h5 class="card-title product-title">                                    <%= product.name %>                                </h5>                                <p class="text-muted brand-name mb-2">                                    <%= product.brand || 'WEARiT' %>                                </p>                                <div                                    class="d-flex justify-content-between align-items-center mb-2"                                >                                    <div class="price-container">                                        <% if (hasDiscount && salePrice <                                        originalPrice) { %>                                        <span class="fw-bold"                                            >₹<%= salePrice.toFixed(2) %></span                                        >                                        <span                                            class="text-decoration-line-through text-muted small"                                            >₹<%= originalPrice.toFixed(2)                                            %></span                                        >                                        <% } else if (defaultVariant) { %>                                        <span class="fw-bold"                                            >₹<%= originalPrice.toFixed(2)                                            %></span                                        >                                        <% } else { %>                                        <span class="fw-bold"                                            >Price not available</span                                        >                                        <% } %>                                    </div>                                    <div class="stock-status">                                        <% if (totalStock <= 5 && totalStock >                                        0) { %>                                        <span class="text-warning small"                                            >Low Stock</span                                        >                                        <% } else if (totalStock <= 0) { %>                                        <span class="text-danger small"                                            >Out of Stock</span                                        >                                        <% } %>                                    </div>                                </div>                                <div class="rating">                                    <% for (var i = 1; i <= 5; i++) { %> <% if                                    (i <= rating) { %>                                    <i                                        class="fas fa-star text-warning small-icon"                                    ></i>                                    <% } else { %>                                    <i                                        class="far fa-star text-warning small-icon"                                    ></i>                                    <% } %> <% } %>                                    <small class="ms-1 text-muted"                                        >(<%= ratingCount %>)</small                                    >                                </div>                            </div>                        </a>                    </div>                </div>                <% }); %> <% } else { %> <% for (var i = 0; i < 4; i++) { %>                <div class="col">                    <div class="card product-card h-100 border-0 shadow-sm">                        <a href="#" class="text-decoration-none">                            <div class="product-img-container">                                <img                                    src="/images/image<%= i+1 %>.jpg"                                    class="card-img-top"                                    alt="Product"                                />                            </div>                            <div class="card-body p-3">                                <h5 class="card-title product-title">                                    Stylish Item                                </h5>                                <p class="text-muted brand-name mb-2">WEARiT</p>                                <div                                    class="d-flex justify-content-between align-items-center mb-2"                                >                                    <span class="fw-bold">₹95.50</span>                                    <span class="text-danger small"                                        >Almost Sold Out</span                                    >                                </div>                                <div class="rating">                                    <% for(var j = 0; j < 5; j++) { %>                                    <i                                        class="fas fa-star text-warning small-icon"                                    ></i>                                    <% } %>                                </div>                            </div>                        </a>                    </div>                </div>                <% } %> <% } %>            </div>            <div class="text-center mt-4">                <a href="/products" class="btn btn-outline-dark"                    >View All New Arrivals</a                >            </div>        </div>    </section>    <section class="nature-banner py-3 py-md-5">        <div class="container-fluid px-0">            <div class="position-relative">                <img                    src="/images/banner1.jpg"                    class="w-100"                    alt="Blend Into Nature"                />                <div                    class="position-absolute top-50 end-0 translate-middle-y pe-3 pe-md-5"                >                    <h2 class="display-6 display-md-4 text-white script-font">                        Blend into nature                    </h2>                </div>            </div>        </div>    </section>    <section class="premium py-4 py-md-5">        <div class="container">            <h2 class="text-center mb-2 mb-md-3">Premium Collection</h2>            <p class="text-center text-muted mb-3 mb-md-5 px-3 px-md-0">                Discover our exclusive premium collection featuring high-quality                fabrics and exceptional craftsmanship. Elevate your style with                these luxury pieces.            </p>            <div class="row row-cols-2 row-cols-md-3 row-cols-lg-4 g-3 g-md-4">                <% if (typeof premiumProducts !== 'undefined' &&                premiumProducts.length > 0) { %> <%                premiumProducts.forEach(function(product) { var mainImage =                product.images.find(img => img.isMain) || product.images[0] || {                url: '/images/placeholder.jpg' }; var defaultVariant =                product.variants && product.variants.length > 0 ?                product.variants[0] : null; var hasDiscount = product.offer > 0                && defaultVariant; var originalPrice = defaultVariant ?                defaultVariant.varientPrice : 0; var salePrice = defaultVariant                ? defaultVariant.salePrice : 0; var totalStock =                product.variants.reduce((sum, variant) => sum +                variant.varientquatity, 0); var rating = product.ratings ?                product.ratings.average : 0; var ratingCount = product.ratings ?                product.ratings.count : 0; %>                <div class="col">                    <div class="card product-card h-100 border-0 shadow-sm">                        <a                            href="/products/<%= product._id %>"                            class="text-decoration-none"                        >                            <div class="product-img-container">                                <img                                    src="<%= mainImage.url %>"                                    class="card-img-top"                                    alt="<%= product.name %>"                                />                            </div>                            <div class="card-body p-3">                                <h5 class="card-title product-title">                                    <%= product.name %>                                </h5>                                <p class="text-muted brand-name mb-2">                                    <%= product.brand || 'WEARiT' %>                                </p>                                <div                                    class="d-flex justify-content-between align-items-center mb-2"                                >                                    <div class="price-container">                                        <% if (hasDiscount && salePrice <                                        originalPrice) { %>                                        <span class="fw-bold"                                            >₹<%= salePrice.toFixed(2) %></span                                        >                                        <span                                            class="text-decoration-line-through text-muted small"                                            >₹<%= originalPrice.toFixed(2)                                            %></span                                        >                                        <% } else if (defaultVariant) { %>                                        <span class="fw-bold"                                            >₹<%= originalPrice.toFixed(2)                                            %></span                                        >                                        <% } else { %>                                        <span class="fw-bold"                                            >Price not available</span                                        >                                        <% } %>                                    </div>                                    <div class="stock-status">                                        <% if (totalStock <= 5 && totalStock >                                        0) { %>                                        <span class="text-warning small"                                            >Low Stock</span                                        >                                        <% } else if (totalStock <= 0) { %>                                        <span class="text-danger small"                                            >Out of Stock</span                                        >                                        <% } %>                                    </div>                                </div>                                <div class="rating">                                    <% for (var i = 1; i <= 5; i++) { %> <% if                                    (i <= rating) { %>                                    <i                                        class="fas fa-star text-warning small-icon"                                    ></i>                                    <% } else { %>                                    <i                                        class="far fa-star text-warning small-icon"                                    ></i>                                    <% } %> <% } %>                                    <small class="ms-1 text-muted"                                        >(<%= ratingCount %>)</small                                    >                                </div>                            </div>                        </a>                    </div>                </div>                <% }); %> <% } else { %> <% for (var i = 0; i < 4; i++) { %>                <div class="col">                    <div class="card product-card h-100 border-0 shadow-sm">                        <a href="#" class="text-decoration-none">                            <div class="product-img-container">                                <img                                    src="/images/image<%= i+1 %>.jpg"                                    class="card-img-top"                                    alt="Product"                                />                            </div>                            <div class="card-body p-3">                                <h5 class="card-title product-title">                                    Premium Item                                </h5>                                <p class="text-muted brand-name mb-2">WEARiT</p>                                <div                                    class="d-flex justify-content-between align-items-center mb-2"                                >                                    <span class="fw-bold">₹195.50</span>                                    <span class="text-danger small"                                        >Limited Edition</span                                    >                                </div>                                <div class="rating">                                    <% for(var j = 0; j < 5; j++) { %>                                    <i                                        class="fas fa-star text-warning small-icon"                                    ></i>                                    <% } %>                                </div>                            </div>                        </a>                    </div>                </div>                <% } %> <% } %>            </div>            <div class="text-center mt-4">                <a                    href="/products?category=<%= premiumProducts && premiumProducts.length > 0 && premiumProducts[0].categoryId ? premiumProducts[0].categoryId._id : '' %>"                    class="btn btn-outline-dark"                    >View All Premium Products</a                >            </div>        </div>    </section>    <section class="split-banner py-3 py-md-5">    <div class="container-fluid px-0">        <div class="row g-0">            <div class="col-md-6 split-banner-text">                <div class="content-wrapper">                    <h2 class="banner-title">The Gentleman's Cut</h2>                    <p class="banner-description">The highest thread count fabrics, from the finest mills in the world.</p>                    <a href="/products" class="btn btn-outline-light">VIEW COLLECTIONS</a>                </div>            </div>            <div class="col-md-6 split-banner-video">                <video autoplay muted loop playsinline>                    <source src="/videos/fabric.mp4" type="video/mp4">                    Your browser does not support the video tag.                </video>            </div>        </div>    </div></section><section class="split-banner py-3 py-md-5">    <div class="container-fluid px-0">        <div class="row g-0">            <div class="col-md-6 split-banner-left">                <div class="content-wrapper">                    <h2 class="banner-title">The OutFit</h2>                    <p class="banner-description">An exclusive membership program for gentlemen with fine taste and even finer style.</p>                    <div class="features">                        <span>10% Off</span>                        <span class="dot-separator">•</span>                        <span>Concierge Services</span>                        <span class="dot-separator">•</span>                        <span>Free Alterations</span>                    </div>                    <div class="art-image">                        <img src="/images/art.png" alt="Stylish men illustration" class="img-fluid">                    </div>                    <a href="/referral" class="btn btn-dark join-btn">REFER YOUR FRIENDS</a>                </div>            </div>            <div class="col-md-6 split-banner-right">                <div class="right-banner-container">                    <img src="/images/rightbanner.png" alt="Mobile app display" class="right-banner-image">                    <div class="app-buttons-overlay">                        <a href="https://www.amazon.in/Alif-Electronics/dp/B0F6T6F99F/ref=mp_s_a_1_1?crid=1OUEN6Y5D4BF7&dib=eyJ2IjoiMSJ9.CThRjNN4fHEIiDoK_LwUBQ.C2rVn82OY0VJWOOGRWWBPZ-On6DpMleO2k_-7Cq6Lxs&dib_tag=se&keywords=alif+electronics&nsdOptOutParam=true&qid=1746348003&s=mobile-apps&sprefix=alif+electronics+%2Caps%2C257&sr=1-1" class="app-button">                            <img src="https://play.google.com/intl/en_us/badges/static/images/badges/en_badge_web_generic.png" alt="Get it on Google Play" class="google-play">                        </a>                        <a href="https://www.amazon.in/Alif-Electronics/dp/B0F6T6F99F/ref=mp_s_a_1_1?crid=1OUEN6Y5D4BF7&dib=eyJ2IjoiMSJ9.CThRjNN4fHEIiDoK_LwUBQ.C2rVn82OY0VJWOOGRWWBPZ-On6DpMleO2k_-7Cq6Lxs&dib_tag=se&keywords=alif+electronics&nsdOptOutParam=true&qid=1746348003&s=mobile-apps&sprefix=alif+electronics+%2Caps%2C257&sr=1-1" class="app-button">                            <img src="https://developer.apple.com/app-store/marketing/guidelines/images/badge-download-on-the-app-store.svg" alt="Download on the App Store" class="app-store">                        </a>                    </div>                </div>            </div>        </div>    </div></section>    <%- include('../partials/footer') %>    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>    <script>        document.addEventListener("DOMContentLoaded", function () {            const heroCarousel = document.getElementById("heroCarousel");            const carousel = new bootstrap.Carousel(heroCarousel, {                interval: 4000,                pause: "hover",                wrap: true,            });        });    </script></body>